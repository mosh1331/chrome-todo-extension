{
  "manifest_version": 3,
  "name": "Sticky To-Do List",
  "description": "A simple sticky note to-do list extension for Chrome.",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}